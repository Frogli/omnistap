Switch pnType
	Case kFieldreference
		Quit method "fieldreference"
	Case kCharacter,kItemref
		Quit method "$is_char"
	Case kBoolean
		Quit method "$is_boolean"
	Case kList
		Quit method "$is_list"
	Case kRow
		Quit method "$is_row"
	Case kNumber,kInteger
		Quit method "$is_number"
	Case kObjectref
		Quit method "$is_objectref"
End Switch