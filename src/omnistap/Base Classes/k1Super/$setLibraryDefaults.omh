Calculate $root.$prefs.$maxcachedclasses as 2500
Calculate $root.$prefs.$printernotify as kFalse
Calculate $root.$prefs.$printernotify as kPrtNoteAuto
Calculate $root.$prefs.$showsystemfocus as kFalse
Calculate $root.$prefs.$usecms as kFalse
Calculate $root.$prefs.$waitforuser as kFalse
Calculate $root.$prefs.$macosvalidatepapersize as kTrue

Do fmtdatetime("m D y h:N A")
Do fmtshortdate("m D y")
Do fmtshorttime("h:N A")